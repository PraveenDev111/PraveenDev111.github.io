			"surface": {
				"backdropFilter": "blur(6px)",
				"background": "transparent",
				"borderColor": "transparent",
				"borderRadius": "var(--wp--custom--border--radius)",
				"borderWidth": "0",
				"boxShadow": "var(--wp--custom--box-shadow)"
