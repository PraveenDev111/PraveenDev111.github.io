			"surfaceOverlay": {
				"background": "var(--wp--preset--color--base)",
				"opacity": "0.5"
			},
			"transition": "all 0.2s ease",
			"videoButton": {
				"background": "var(--wp--preset--gradient--primary),#fff"
			}
		},
		"layout": {
			"contentSize": "min(calc(100dvw - 3rem), 800px)",
			"wideSize": "min(calc(100dvw - 3rem), 1200px)"
		},
		"spacing": {
			"customSpacingSize": true,
			"spacingScale": {
				"increment": 0,
				"mediumStep": 0,
				"operator": "*",
				"steps": 0,
				"unit": "px"
			},
			"spacingSizes": [
				{
					"name": "XXS",
					"size": "calc(var(--wp--preset--font-size--medium,1rem)/2)",
					"slug": "xxs"
				},
				{
					"name": "XS",
					"size": "var(--wp--preset--font-size--medium,1rem)",
					"slug": "xs"
				},
				{
					"name": "S",
